<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.NewStep" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Customize your dark theme here. -->
        <item name="background">@color/blueDark</item>
        <item name="backgroundColor">@color/black</item>
        <item name="color">@color/blueDark</item>



        <!-- <item name="colorPrimary">@color/my_dark_primary</item> -->
    </style>

    <style name="Theme.NewStep" parent="Base.Theme.NewStep" />



    <style name="Screan">
        <item name="android:background">@color/black</item>
    </style>

    <style name="Text">
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="Image">
        <item name="tint">@color/white</item>
    </style>
    <style name="Widget.MyApp.NavigationView" parent="Widget.MaterialComponents.NavigationView">
        <item name="android:backgroundTint">@color/black</item>
        <item name="itemIconTint">@color/blueDark</item>
        <item name="itemTextColor">@color/blueDark</item>
        <item name="itemRippleColor">@color/blue</item>
        <item name="subheaderColor">@color/white</item>
        <item name="itemIconSize">@dimen/_20sdp</item>
        <item name="itemTextAppearanceActiveBoldEnabled">true</item>
    </style>

    <style name="BlueColorToolbar" parent="Widget.AppCompat.Toolbar">
        <item name="android:background">@color/blueDark</item>
        <item name="titleTextColor">@color/white</item>
    </style>
    <style name="BlueColor" >
        <item name="android:background">@color/blueDark</item>
    </style>
    <style name="TextSec" >
        <item name="android:textColor">@color/light_blue</item>
    </style>
    <style name="Card" >
        <item name="cardBackgroundColor">@color/blueDark</item>

    </style>
    <style name="purpleColor" >
        <item name="android:background">@color/DarkPurple</item>

    </style>
    <style name="groups_items_colors" >
        <item name="android:background">@color/black</item>

    </style>

    <style name="icons" >
        <item name="tint">@color/white</item>
    </style>





</resources>